{
    "extends": [
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "semi": true,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  }
  